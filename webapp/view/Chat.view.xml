<mvc:View xmlns:l="sap.ui.layout" 
	xmlns:core="sap.ui.core" 
	xmlns:mvc="sap.ui.core.mvc" 
	xmlns="sap.m" 
	xmlns:tnt="sap.tnt"
	controllerName="DrThinkingPatient.controller.Chat">
	<App id="app">
		<Page 
			title="{i18n>title}"
			showHeader="false" >
			<content>
				<Panel>
							<List headerText="Chatting list">
								<items>
									<ObjectListItem title="Daniel Thinking" type="Active" press="onListItemPress" icon="sap-icon://wounds-doc">
										<firstStatus icon="sap-icon://email">
											<ObjectStatus text="yesterday" icon="sap-icon://email"/>
										</firstStatus>
										<attributes>
											<ObjectAttribute text="status:offline" />
										</attributes>
									</ObjectListItem>
									<ObjectListItem title="Tai Ly" type="Active" press="onListItemPress"
									icon="sap-icon://wounds-doc">
										<firstStatus>
											<ObjectStatus text="yesterday" icon="sap-icon://email"/>
										</firstStatus>
										<attributes>
											<ObjectAttribute text="status:online"/>
											
										</attributes>
									</ObjectListItem>
									<ObjectListItem title="Hossam" type="Active" press="onListItemPress"
										icon="sap-icon://wounds-doc">
										<firstStatus>
											<ObjectStatus text="Oct 11" icon="sap-icon://email"/>
											
										</firstStatus>
										<attributes>
											<ObjectAttribute text="status:online"/>
										</attributes>
									</ObjectListItem>
									<ObjectListItem title="Tom" type="Active" press="onListItemPress"
										icon="sap-icon://wounds-doc">
										<firstStatus>
											<ObjectStatus text="Oct 10" icon="sap-icon://email"/>
										
										</firstStatus>
										<attributes>
											<ObjectAttribute text="status:offline"/>
										</attributes>
									</ObjectListItem>
								</items>
							</List>
				</Panel>
			</content>
		</Page>
	</App>
</mvc:View>