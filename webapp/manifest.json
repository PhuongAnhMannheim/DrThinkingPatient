{
	"_version": "1.7.0",
	"sap.app": {
		"id": "DrThinkingPatient",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": [
			"sap_hcb",
			"sap_belize"
		]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "DrThinkingPatient.view.App",
			"type": "XML"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {},
				"sap.ushell": {},
				"sap.collaboration": {},
				"sap.ui.comp": {},
				"sap.uxap": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "DrThinkingPatient.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "DrThinkingPatient.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "appWelcome",
					"target": "welcome"
				},
				{
					"pattern": "register",
					"name": "register",
					"target": "register"
				},
				{
					"pattern": "profile",
					"name": "profile",
					"target": "profile"
				},
				{
					"pattern": "home",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "search",
					"name": "search",
					"target": "search"
				},
				{
					"pattern": "appointment",
					"name": "appointment",
					"target": "appointment"
				},
				{
					"pattern": "match",
					"name": "match",
					"target": "match"
				},
				{
					"pattern": "chat",
					"name": "chat",
					"target": "chat"
				},
				{
					"pattern": "chatroom",
					"name": "chatroom",
					"target": "chatroom"
				}
			],
			"targets": {
				"welcome": {
					"viewName": "Welcome"
				},
				"register": {
					"viewName": "Register"
				},
				"profile": {
					"viewName": "Profile"
				},
				"home": {
					"viewName": "Home"
				},
				"search": {
					"viewName": "Search"
				},
				"appointment": {
					"viewName": "Appointment"
				},
				"match": {
					"viewName": "Match"
				},
				"chat": {
					"viewName": "Chat"
				},
				"chatroom": {
					"viewName": "Chatroom"
				}
			}
		}
	},
	"sap.platform.hcp": {
		"uri": "webapp",
		"_version": "1.1.0"
	}
}